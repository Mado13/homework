<div class="box-cal" style="width: 45%;height:300px;">
  <%= simple_form_for [@space, @booking], method: "post" do |f| %>
  <div class="form-inputs w-50">
    <%= f.input :start_date, as: :string, label: false, placeholder: "Check-In", input_html: {class: "datepicker"} %>
    <%= f.input :end_date, as: :string, label: false,  placeholder: "Check-Out", input_html: {class: "datepicker"} %>
    <%= f.button :submit, value: "Book Space", class: "button-bookspace" %>
  </div>
  <% end %>
</div>
