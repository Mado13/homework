 <%= simple_form_for [@booking, @review], remote: true, url: booking_reviews_path(booking),
                      method: "POST" do |f|  %>
  <%= f.text_area :comment, placeholder: "Write your review here..." %>
  <%= f.input :rating, collection: (1..10).to_a, label: "Rate your experience" %>
  <%= f.submit %>
    <% bidning.pry %>
 <% end %>
